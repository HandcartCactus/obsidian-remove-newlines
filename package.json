{
	"name": "obsidian-remove-newlines",
	"version": "1.0.0",
	"description": "This plugin adds context menu options and commands to:\n 1. Remove newlines from text selections.\n 2. Paste content from the clipboard without newlines.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "npx jest --config ./jest.config.js"
	},
	"keywords": [
		"newlines",
		"formatting",
		"paste",
		"obsidian"
	],
	"author": "Elias Jaffe",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "^0.25.0",
		"jest": "^29.7.0",
		"obsidian": "latest",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
